                        <RowConstraints vgrow="NEVER" />
                        <RowConstraints vgrow="NEVER" />
                        <RowConstraints vgrow="NEVER" />
                        <RowConstraints vgrow="NEVER" />
                        <RowConstraints vgrow="NEVER" />
                     </rowConstraints>
                     <children>
                        <ComboBox fx:id="mButtonsMappingCombo" prefWidth="200.0" GridPane.columnIndex="1" />
                        <Label text="%controls" />
                        <Label text="%controls.port" GridPane.rowIndex="1" />
                        <ComboBox fx:id="mControllersCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <ComboBox fx:id="mControllerModeCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
