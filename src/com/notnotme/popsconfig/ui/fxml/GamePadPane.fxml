                  <ComboBox fx:id="mLStickDownButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="13" />
                  <ComboBox fx:id="mRStickLeftButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="14" />
                  <ComboBox fx:id="mRStickRightButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="15" />
                  <ComboBox fx:id="mRStickUpButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="16" />
                  <ComboBox fx:id="mRStickDownButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="17" />
               </children>
            </GridPane>
         </content>
