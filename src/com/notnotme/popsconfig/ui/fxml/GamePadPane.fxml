<javafx.scene.control.TabPane maxHeight="-Infinity" maxWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.notnotme.popsconfig.controller.GamePadPaneController">
   <tabs>
      <Tab closable="false" text="%tab_main">
         <content>
            <VBox spacing="5.0">
               <children>
                  <GridPane hgap="5.0" vgap="5.0">
                     <columnConstraints>
                        <ColumnConstraints hgrow="ALWAYS" />
                        <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" />
