              </rowConstraints>
               <children>
                  <Label text="%screen.custom.x" />
                  <Label text="%screen.custom.y" GridPane.rowIndex="1" />
                  <Label text="%screen.custom.width" GridPane.rowIndex="2" />
                  <Label text="%screen.custom.height" GridPane.rowIndex="3" />
                  <Spinner fx:id="mCustomScreenXSprinner" GridPane.columnIndex="1" />
                  <Spinner fx:id="mCustomScreenYSprinner" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Spinner fx:id="mCustomScreenWidthSprinner" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Spinner fx:id="mCustomScreenHeightSprinner" GridPane.columnIndex="1" GridPane.rowIndex="3" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </GridPane>
