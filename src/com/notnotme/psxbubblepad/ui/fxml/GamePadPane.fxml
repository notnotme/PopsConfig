<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<javafx.scene.control.TabPane maxHeight="-Infinity" maxWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.notnotme.psxbubblepad.controller.GamePadPaneController">
   <tabs>
      <Tab closable="false" text="%tab_controls">
         <content>
            <GridPane vgap="5.0">
              <columnConstraints>
                <ColumnConstraints hgrow="ALWAYS" />
                <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints vgrow="NEVER" />
                <RowConstraints vgrow="NEVER" />
                <RowConstraints vgrow="NEVER" />
              </rowConstraints>
               <children>
                  <ComboBox fx:id="mCustomButtonsCombo" prefWidth="200.0" GridPane.columnIndex="1" />
                  <Label text="%controls" />
                  <Label text="%port" GridPane.rowIndex="1" />
                  <ComboBox fx:id="mControllersCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <ComboBox fx:id="mModesCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label text="%mode" GridPane.rowIndex="2" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </GridPane>
         </content>
      </Tab>
      <Tab fx:id="mCustomControlsTab" closable="false" text="%tab_custom_controls">
         <content>
            <GridPane vgap="5.0">
              <columnConstraints>
                <ColumnConstraints hgrow="ALWAYS" />
                <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
              </rowConstraints>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                  <Label text="%up" />
                  <Label text="%down" GridPane.rowIndex="1" />
                  <Label text="%left" GridPane.rowIndex="2" />
                  <Label text="%right" GridPane.rowIndex="3" />
                  <Label text="%l" GridPane.rowIndex="4" />
                  <Label text="%r" GridPane.rowIndex="5" />
                  <Label text="%cross" GridPane.rowIndex="6" />
                  <Label text="%square" GridPane.rowIndex="7" />
                  <ComboBox fx:id="mUpButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" />
                  <ComboBox fx:id="mDownButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <ComboBox fx:id="mLeftButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <ComboBox fx:id="mRightButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <ComboBox fx:id="mLButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <ComboBox fx:id="mRButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <ComboBox fx:id="mCrossButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                  <ComboBox fx:id="mSquareButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                  <Label text="%circle" GridPane.rowIndex="8" />
                  <Label text="%triangle" GridPane.rowIndex="9" />
                  <Label text="%analog.left.left" GridPane.rowIndex="10" />
                  <Label text="%analog.left.right" GridPane.rowIndex="11" />
                  <ComboBox fx:id="mCircleButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="8" />
                  <ComboBox fx:id="mTriangleButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="9" />
                  <Label text="%analog.left.up" GridPane.rowIndex="12" />
                  <Label text="%analog.left.down" GridPane.rowIndex="13" />
                  <Label text="%analog.right.left" GridPane.rowIndex="14" />
                  <Label text="%analog.right.right" GridPane.rowIndex="15" />
                  <Label text="%analog.right.up" GridPane.rowIndex="16" />
                  <Label text="%analog.right.down" GridPane.rowIndex="17" />
                  <ComboBox fx:id="mLStickLeftButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="10" />
                  <ComboBox fx:id="mLStickRightButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="11" />
                  <ComboBox fx:id="mLStickUpButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="12" />
                  <ComboBox fx:id="mLStickDownButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="13" />
                  <ComboBox fx:id="mRStickLeftButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="14" />
                  <ComboBox fx:id="mRStickRightButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="15" />
                  <ComboBox fx:id="mRStickUpButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="16" />
                  <ComboBox fx:id="mRStickDownButtonCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="17" />
               </children>
            </GridPane>
         </content>
      </Tab>
      <Tab closable="false" text="%tab_front_touch">
         <content>
            <GridPane vgap="5.0">
              <columnConstraints>
                <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints vgrow="NEVER" />
                <RowConstraints vgrow="NEVER" />
                <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
              </rowConstraints>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                  <Label text="%touch.upper.left" />
                  <Label text="%touch.upper.right" GridPane.rowIndex="1" />
                  <Label text="%touch.bottom.left" GridPane.rowIndex="2" />
                  <Label text="%touch.bottom.right" GridPane.rowIndex="3" />
                  <ComboBox fx:id="mTouchUpperLeftCombo" prefWidth="200.0" GridPane.columnIndex="1" />
                  <ComboBox fx:id="mTouchUpperRightCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <ComboBox fx:id="mTouchBottomLeftCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <ComboBox fx:id="mTouchBottomRightCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
               </children>
            </GridPane>
         </content>
      </Tab>
      <Tab closable="false" text="%tab_rear_touch">
         <content>
            <GridPane vgap="5.0">
              <columnConstraints>
                <ColumnConstraints hgrow="ALWAYS" />
                <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints vgrow="NEVER" />
                <RowConstraints vgrow="NEVER" />
                <RowConstraints vgrow="NEVER" />
                  <RowConstraints vgrow="NEVER" />
              </rowConstraints>
               <children>
                  <Label text="%touch.rear.upper.left" />
                  <ComboBox fx:id="mRearTouchUpperLeftCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" />
                  <Label text="%touch.rear.upper.right" GridPane.rowIndex="1" />
                  <ComboBox fx:id="mRearTouchUpperRightCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label text="%touch.rear.bottom.left" GridPane.rowIndex="2" />
                  <ComboBox fx:id="mRearTouchBottomLeftCombo" prefHeight="26.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label text="%touch.rear.bottom.right" GridPane.rowIndex="3" />
                  <ComboBox fx:id="mRearTouchBottomRightCombo" prefWidth="200.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </GridPane>
         </content>
      </Tab>
   </tabs>
</javafx.scene.control.TabPane>
